{
    "name": "oro/commerce-apruve",
    "description": "OroCommerce Apruve Integration",
    "homepage": "https://github.com/orocommerce/OroApruveBundle.git",
    "license": "OSL-3.0",
    "keywords": ["Oro", "Apruve", "Integration"],
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "https://www.orocommerce.com/"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\ApruveBundle\\": ""},
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "oro/commerce": "3.1.*"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
